{
  "name": "agmob-driver",
  "version": "1.0.0",
  "description": "AgMob Driver application",
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "club.itsp.elang.agmob.driver",
    "files": [
      "build/**/*"
    ],
    "protocols": [
      {
        "name": "agmob-driver",
        "schemes": [
          "agmob-driver"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "perMachine": true
    }
  },
  "scripts": {
    "start-react-scripts": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "node_modules/.bin/electron --enable-transparent-visuals .",
    "start": "nf start -p 3000",
    "dist": "npm run build && electron-builder -mwl"
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-bootstrap": "^0.32.19",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "bootstrap": "^4.3.1",
    "foreman": "^3.0.1",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.0",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "typescript": "^3.5.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.0.15",
    "tslint": "^5.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
